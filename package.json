{
  "name": "fftool",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:gh-pages": "npm run data:refresh && npm run build",
    "preview:dist": "vite preview --port 4173",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "integrity:baseline": "tsx scripts/generateIntegrityBaseline.ts",
    "integrity:verify": "tsx scripts/verifyIntegrity.ts",
    "integrity:check": "tsx scripts/verifyIntegrity.ts",
    "integrity:update": "tsx scripts/updateIntegrityBaseline.ts",
    "integrity:detect-new": "tsx scripts/detectNewFiles.ts",
    "pipeline:safe": "npm run integrity:verify && npm run pipeline:run && npm run integrity:verify",
    "etl:load": "python3 etl/robust_loader.py",
    "etl:pipeline": "python3 etl/pipeline.py",
    "etl:pipeline:safe": "npm run integrity:verify && python3 etl/pipeline.py",
    "data:refresh": "npm run etl:pipeline:safe",
    "data:check": "python3 -c \"import json; m=json.load(open('artifacts/data_manifest.json')); print(f'Data age: {m.get('generated_at')}, Files: {len(m.get('files',{}))}, Verified: {m.get('integrity_verified')}')\"",
    "data:register": "tsx scripts/registerDataSource.ts",
    "data:register-new": "tsx scripts/registerDataSource.ts",
    "data:update-all": "bash scripts/weeklyUpdate.sh",
    "weekly:update": "bash scripts/weeklyUpdate.sh",
    "validate:complete": "python3 validation/validate_data_complete.py",
    "predev": "npm run data:check || npm run data:refresh",
    "pipeline:run": "python3 etl/pipeline.py"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.1",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.424.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.4.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "csv-parse": "^6.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.20.5",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
